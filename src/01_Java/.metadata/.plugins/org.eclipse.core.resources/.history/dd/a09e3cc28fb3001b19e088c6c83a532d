package com.lec.ex;

import java.util.Scanner;

public class Test {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int dansu;
		do {
			System.out.print("원하는 단수는(단, 2~9단만 가능)?");
			dansu = sc.nextInt();
		}while(dansu<2 || dansu>9);
		gugudan(dansu);
		sc.close();
	}
}
